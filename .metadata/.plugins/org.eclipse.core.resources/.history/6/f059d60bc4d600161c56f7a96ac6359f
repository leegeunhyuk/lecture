import java.util.Scanner;

public class OtherExam02 {

	public static void main(String[] args) {
		int [] arr = new int[55];
		int num = 0;
		for(int i = 0; i < 55; i++){
			arr[i] = i + 1;
		}
		for(int i = 0; i < 10; i++){
			for(int j = 10 - i - 1; j > 10; j--){
				System.out.print("     ");
			}
		}
		
	}
}
