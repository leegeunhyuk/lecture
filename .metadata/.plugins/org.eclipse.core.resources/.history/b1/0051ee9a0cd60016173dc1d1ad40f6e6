import java.util.Scanner;

public class ArrayExam01 {

	public static void main(String[] args) {
		Scanner keyIn = new Scanner(System.in);
		
		int [] ary = new int[5];
		
		for(int i = 0; i < 5; i++){
			System.out.print(i + "번째 요소 : ");
			ary[i] = keyIn.nextInt();
		}
		for(int i = 0; i < 5; i++){
			System.out.print(ary[i] + "\t");
		}
		
		
		keyIn.close();
	}
}
