import java.util.Scanner;

public class OtherExam02 {

	public static void main(String[] args) {
		int [] arr = new int[55];
		int num = 0;
		for(int i = 0; i < 55; i++){
			arr[i] = i + 1;
		}
		for(int i = 0; i < 10; i++){
			for(int j = 10 - i -1; j  > 0; j-- ){
				System.out.print("     ");
			}
			for(int j = 0; j < 55; j++){
				System.out.printf("%5d", arr[num]);
				num++

//				System.out.printf("%5d", arr[j]);
//				num ++;
//				if(num > i) { break; }
			}
			System.out.println();
		}
	}
}
